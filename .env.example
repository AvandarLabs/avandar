################################################################################
# Public environment variables.
# These get sent to the client's browser.
################################################################################
VITE_APP_URL=http://localhost:5173/
VITE_SUPABASE_API_URL=
VITE_SUPABASE_ANON_KEY=
VITE_HIDE_DEV_TOOLS=false

# Flags to enable. Must be a semi-colon separated list.
# See `FeatureFlagConfig.tsx` for available flags.
# Example: VITE_FEATURE_FLAGS=disable-self-registration
VITE_FEATURE_FLAGS=

# Google Picker API
VITE_GOOGLE_PICKER_API_KEY=

################################################################################
# Private environment variables.
# These environment variables are never served to the frontend.
################################################################################
SUPABASE_SERVICE_ROLE_KEY=