{
  "name": "avandar",
  "license": "CPAL-1.0",
  "private": true,
  "version": "0.4.0-dev",
  "type": "module",
  "scripts": {
    "dev": "./scripts/startAvandar.sh",
    "dev:email": "dotenv -e .env.development -- email dev",
    "test": "vitest",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "type-check": "tsc -b --noEmit",
    "preview": "vite preview",
    "current-version": "cat package.json | grep --color=auto \"\\\"version\\\":\"",
    "tsc-watch": "concurrently --raw \"nodemon --ext ts,tsx --watch supabase/functions --exec 'deno check supabase/functions --quiet && echo \\\"\\033[32mType check passed in supabase/functions\\033[0m\\\" || true' 2>&1 | grep -v '\\[nodemon\\]'\" \"FORCE_COLOR=1 npm run tsc -b --watch tsconfig.app.json tsconfig.node.json\"",
    "db:apply-migrations": "./scripts/applyMigrations.sh",
    "db:create-user": "npm run vite-script scripts/createUserScript.ts",
    "db:gen-types": "supabase gen types typescript --local > src/types/database.types.ts",
    "db:new-migration": "bash -c 'supabase stop && PGSSLMODE=disable supabase db diff -f \"$1\"' --",
    "db:down-migration": "./scripts/downMigration.sh",
    "db:redo-migration": "./scripts/redoLastMigration.sh --gen-types",
    "db:reset": "supabase start && ./scripts/resetDatabase.sh",
    "db:seed": "supabase start && npm run vite-script scripts/seedDatabaseScript.ts",
    "env:reset": "cp .env.example .env.development && cp .env.example.edge .env.development.edge",
    "new:component": "./scripts/newComponent.sh",
    "new:model": "./scripts/newModel.sh",
    "new:route": "./scripts/newRoute.sh",
    "new:script": "./scripts/newScript.sh",
    "new:fn": "./scripts/edge-functions/newEdgeFunction.sh",
    "fns:call": "./scripts/edge-functions/callFunction.sh",
    "fns:update-env": "cp .env.development.edge supabase/functions/.env",
    "fns:serve": "supabase functions serve",
    "vite-script": "vite-node --config ./vite.script.config.ts",
    "util:get-project-root": "./scripts/utils/getProjectRoot.sh"
  },
  "//comments:dependencies": {
    "@polar-sh/sdk": "Added for Deno type-checking in Supabase edge functions",
    "apache-arrow": "This needs to be the same version of apache-arrow used by duckdb-wasm."
  },
